                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>

                                        @* Set Dashboard *@
                                        <div class="col-lg-5 col-md-5 c-setting__width-box-list">
                                            <div class="c-setting__box-list">
                                                <div class="c-setting__header-list">
                                                    Dashboard Set
                                                </div>

                                                <div id="setDashboard" class="c-setting__scroll-box-list">
                                                    @*<ul id="list-dashboard-set" class="c-setting__list-set">
                                                            @* must be empty
                                                        </ul>*@

                                                    <ul id="list-dashboard-set" class="c-setting__list-set">
                                                        @foreach (var item in Model.ListDashboardSet)
                                                        {
                                                            <li data-value="@item.Id">
                                                                @item.Name
                                                                <a href="~/Images/shimano-white-logo.png" class="preview pull-right"><i class="fas fa-eye"></i></a>
                                                            </li>
                                                        }
                                                    </ul>

                                                </div>
                                            </div>
                                        </div>
                                    </div>


                                    <script>
                                        function SaveData() {
                                            var list = document.getElementById('list-dashboard-set');
                                            var list_items = list.getElementsByTagName("li");
                                            var listApp = document.getElementById('list-application-set');
                                            var listApp_items = listApp.getElementsByTagName("li");
                                            var StrData = "";

                                            try {
                                                for (var i = 0; i <= list_items.length ; ++i) {
                                                    StrData = list_items[i].getAttribute('data-value') + "," + StrData
                                                }
                                            }
                                            catch (err) { }

                                            try {
                                                for (var i = 0; i <= listApp_items.length ; ++i) {
                                                    StrData = listApp_items[i].getAttribute('data-value') + "," + StrData
                                                }
                                            }
                                            catch (err) { }


                                            //alert(StrData);

                                            $.ajax({
                                                url: "@Url.Action("SaveData", "Main")",
                                                dataType: "json",
                                                data: { StrData: StrData },
                                                success: function (response) {
                                                if (response.isSuccess) {
                                                    alert(response.message);
                                                    window.location.href = "/Main/Index";
                                                } else {
                                                    // DoSomethingElse()
                                                    alert(response.message);
                                                }
                                            },
                                            error: function (response) {
                                                alert("error!");  //
                                            }
                                        });
                                        }
                                    </script>


                                    @* Submit & Cancel *@
                                    <div class="row c-setting__mrg-top-box-confirm">
                                        <div class="col-lg-10 col-md-10 width-space-area-btn-setting"></div>
                                        <div class="col-lg-1 col-md-1 width-btn-cancel-setting">
                                            <div data-dismiss="modal" class="c-btn-circle__box-cancel c-setting__float-btn-confirm">
                                                <div class="c-btn-circle__box-icon">
                                                    <div class="c-btn-circle__icon">
                                                        <i class="fas fa-times"></i>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>

                                        <div class="col-lg-1 col-md-1 width-btn-confirm-setting">
                                            <div data-dismiss="modal" class="c-btn-circle__box c-setting__float-btn-confirm">
                                                <div class="c-btn-circle__box-icon">
                                                    <div class="c-btn-circle__icon">
                                                        <span onclick="SaveData()">
                                                            <i class="fas fa-check"></i>
                                                        </span>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>

