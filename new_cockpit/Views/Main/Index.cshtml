<script>

    function onSuccess(data) {
        if (data.isSuccess) {
            //showMyTask();
            alert(data.message);
        } else {
            alert(data.message);
        }

    }

    function onFailure(data) {
        alert(data.message);
    }



    function Login() {
        ShowLoading(true, "trying to login");
        var UserId = document.getElementById("tbUserID").value;
        var Password = document.getElementById("tbPassword").value;
        $.ajax({
            url: "@Url.Action("LoginUser", "Main")",
            dataType: "json",
            data: {
                UserId: UserId,
                Password: Password
            },
            success: function (response) {
                ShowLoading(false,"");
                if (response.isSuccess) {
                    alert(response.message);
                    location.reload();
                } else {
                    // DoSomethingElse()
                    alert(response.message);
                }
            },
            error: function (response) {
                ShowLoading(false, "");
                alert("error!");  //
            }
        });
    }
</script>

