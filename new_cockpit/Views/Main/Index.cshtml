                                                    <div id="move_right" class="c-setting__mrg-bottom-btn-arrow">
                                                        <div class="c-btn-circle__box position-c">
                                                            <div class="c-btn-circle__box-icon">
                                                                <div class="c-btn-circle__icon">
                                                                    <i class="fas fa-chevron-right"></i>
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>

                                                    @* Btn Arrow To Left *@
                                                    <div id="move_left">
                                                        <div class="c-btn-circle__box position-c">
                                                            <div class="c-btn-circle__box-icon">
                                                                <div class="c-btn-circle__icon">
